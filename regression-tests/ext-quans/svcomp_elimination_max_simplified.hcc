void main() {
    int n = 3;

    int a[_];
    int x = 0;
    int y = n - 1;
    int ax = 0;
    int ay = 0;
    int last = 0;

    while(x < y) {
        ax = a[x];
        ay = a[y];
        if(ax <= ay) {
            x++;
            last = ay;
        }
        else {
            y--;
            last = ax;
        }
    }

    assert(\max(a, 0, n) == last);
}
