#!/bin/sh



TESTS="nonlinear2.hcc \
       ../horn-hcc/loop1.hcc \
       ../horn-hcc/loop2.hcc \
       bitwise1.hcc \
       bitwise2.hcc"

for name in $TESTS; do
    echo
    echo $name
    $TRI_CMD -cex -abstract:off -arithMode:ilp32 "$@" $name 2>&1 | grep -v 'at '
done

echo
echo MonadicInv.hcc
$TRI_CMD -arithMode:ilp32 "$@" MonadicInv.hcc 2>&1

echo
echo bitwise3.hcc
$TRI_CMD -arithMode:ilp32 "$@" bitwise3.hcc 2>&1

